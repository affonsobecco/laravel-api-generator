{
    "name": "julioalves/laravel-api-generator",
    "description": "Laravel API/Scaffold/CRUD Generator from just one command with including Controller, Repository, Model, Migrations, web.php update.",
    "keywords": [
        "laravel",
        "api",
        "model",
        "service",
        "interface",
        "validator",
        "migration",
        "scaffold",
        "CRUD",
        "generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Julio Alves",
            "email": "julio.cesar.alves@gmail.com"
        },

        {
            "name": "Affonso Becco",
            "email": "affonsobecco@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "5.4.*",
        "prettus/l5-repository": "^2.6.*",
        "prettus/laravel-validation": "^1.1.*"

    },
    "autoload": {
        "psr-4": {
            "Becco\\": "src/Becco/"
        }
    },
    "suggest": {
        "doctrine/dbal": "Required to use generator from existing table."
    }
}
